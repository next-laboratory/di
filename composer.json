{
  "name": "next/di",
  "description": "A lightweight dependency injection tool, container based on the PSR-11 specification.",
  "homepage": "https://github.com/marxphp/di",
  "license": "Apache-2.0",
  "keywords": [
    "container",
    "nextphp",
    "dependence injection"
  ],
  "authors": [
    {
      "name": "chengyao",
      "email": "987861463@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Next\\Di\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
